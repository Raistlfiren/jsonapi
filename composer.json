    "type": "bolt-extension",
    "keywords": [
        "json",
        "api",
        "jsonapi"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Dammers",
            "email": "tobias@twokings.nl"
        },
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        },
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao@twokings.nl"
        },
        {
            "name": "Dennis Snijder",
            "email": "Dennissnijder97@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\JsonApi\\": [
                "",
                "src"
            ]
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bolt\\JsonApi\\Extension"
    }
}
